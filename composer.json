{
  "name": "knot-php/module-knot-logger",
  "description": "knot-lib/logger installer modules",
  "keywords": ["PHP","Logger","Module","Installer","kNot Framework"],
  "homepage": "https://github.com/knot-php/module-knot-logger",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Katsuki Shuto",
      "email": "stk2k@knot-framework.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "psr/log": "^1.0",
    "stk2k/eventstream": "~0.7",
    "knot-lib/logger": "~0.1",
    "knot-lib/kernel": "~0.1",
    "knot-lib/module": "~0.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.3.0",
    "php-coveralls/php-coveralls": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "KnotPhp\\Module\\KnotLogger\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "KnotPhp\\Module\\KnotLogger\\Test\\": "test/include/",
      "KnotPhp\\Module\\KnotLogger\\Demo\\": "demo/include/"
    }
  }
